<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.karen.webservices.MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:text="consultar"
        android:layout_width="376dp"
        android:layout_height="40dp"
        android:id="@+id/consultar"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/consultarid"
        android:layout_marginTop="8dp"
        android:ems="0"/>

    <Button
        android:text="Consular ID"
        android:layout_width="376dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/consultarid"
        tools:layout_editor_absoluteY="0dp"
        android:ems="0"/>

    <Button
        android:text="insertar"
        android:layout_width="383dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/insertar"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/consultar"
        android:ems="0"/>

    <Button
        android:text="borrar"
        android:layout_width="384dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/borrar"
        app:layout_constraintTop_toBottomOf="@+id/insertar"
        android:layout_marginTop="8dp"
        android:ems="0"/>

    <Button
        android:text="Actualizar"
        android:layout_width="384dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/actualizar"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/borrar"
        android:ems="0"/>


    <EditText
        android:layout_width="384dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:hint="Nombre"
        android:ems="10"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/enombre"
        app:layout_constraintTop_toBottomOf="@+id/eidentificador"
        android:layout_marginTop="8dp" />

    <EditText
        android:layout_width="384dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:hint="Resultado"
        android:ems="10"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/tresultado"
        app:layout_constraintTop_toBottomOf="@+id/edireccion"
        android:layout_marginTop="8dp" />

    <EditText
        android:layout_width="384dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:hint="Direccion"
        android:ems="10"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/edireccion"
        app:layout_constraintTop_toBottomOf="@+id/enombre"
        android:layout_marginTop="8dp" />

    <EditText
        android:layout_width="384dp"
        android:layout_height="42dp"
        android:inputType="textPersonName"
        android:hint="ID"
        android:ems="10"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/eidentificador"
        app:layout_constraintTop_toBottomOf="@+id/actualizar"
        android:layout_marginTop="8dp" />

</android.support.constraint.ConstraintLayout>
